{
  "name": "eslint-plugin-uu5",
  "version": "1.1.1",
  "description": "Eslint plugin for uu5 library",
  "license": "License Commercial",
  "spec": "nodejs-lib; uu_appg01_devkit@4.3.0",
  "dependencies": {
