{
  "name": "uu5g05-forms",
  "version": "1.26.5",
  "description": "The library contains basic inputs and form which are connected to uuGds.",
  "license": "License Commercial",
  "namespace": "Uu5Forms",
  "spec": "uu5-lib; uu_appg01_devkit@5.17.0",
  "dependencies": {
