{
  "name": "uu5tilesg02",
  "version": "2.11.0",
  "description": "Library contains components for virtual rendering of list of data. It is possible to filter, sort, search, select and moving data by drag and drop.",
  "license": "License Commercial",
  "namespace": "Uu5Tiles",
  "spec": "uu5-lib; uu_appg01_devkit@5.17.0",
  "dependencies": {
    "uu5g05": "^1.0.0"
  },
  "uuBuildSettings": {
    "packs": [
      {
        "entryPoints": [
          "index.js"
        ],
        "libraryGlobalVariable": "Uu5Tiles",
        "outputFile": "uu5tilesg02.js"
      },
      {
        "entryPoints": [
          "config/example-config.js"
        ],
        "outputFile": "assets/example-config.js",
        "hot": false,
        "minify": false
      }
    ]
  },
  "devDependencies": {
    "properties-reader": "0.0.16",
    "uu_appg01": "^5.0.0",
    "uu_appg01_devkit": "^5.17.0",
    "uu_appg01_oidc": "^3.0.0",
    "uu5g05-test": "^1.0.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "clean": "uu_appg01_devkit clean",
    "test": "uu_appg01_devkit test",
    "build": "uu_appg01_devkit build",
    "package": "uu_appg01_devkit package",
    "dist": "uu_appg01_devkit package",
    "start": "uu_appg01_devkit start",
    "prettify": "uu_appg01_devkit prettify",
    "generate": "uu_appg01_devkit generate",
    "uuAppDevKitUpgrade": "uu_appg01_devkit uuAppDevKitUpgrade",
    "docUpdateExamples": "node tools/doc-cmd/update.js examples",
    "docUpdateSources": "node tools/doc-cmd/update.js sources",
    "docUploadFiles": "node tools/doc-cmd/update.js files",
    "lsiExport": "uu_appg01_devkit lsiExport",
    "lsiImport": "uu_appg01_devkit lsiImport"
  },
  "main": "index.js",
  "prettier": {
    "printWidth": 120
  },
  "jest": {
    "transformIgnorePatterns": [
      "^(?!.*/node_modules/(uu_appg01|@?react-dnd|dnd-core)).*/node_modules/"
    ]
  },
  "typings": "index.d.ts"
}